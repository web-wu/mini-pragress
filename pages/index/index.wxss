.swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper swiper image {
  width: 100%;
}
.navBar {
  display: flex;
}
.navBar navigator {
  flex: 1;
  padding: 20rpx;
}
.navBar navigator image {
  width: 100%;
}
.infoBar .floor_group .floor_title image {
  width: 100%;
  padding: 10rpx 0;
}
.infoBar .floor_group .floor_content {
  overflow: hidden;
}
.infoBar .floor_group .floor_content navigator {
  float: left;
  width: 33.33%;
}
.infoBar .floor_group .floor_content navigator image {
  width: 100%;
  height: 100%;
}
.infoBar .floor_group .floor_content navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.infoBar .floor_group .floor_content navigator:nth-child(2),
.infoBar .floor_group .floor_content navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
